    <style name="Theme.GithubAccount" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" ns1:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="textAppearanceHeadline1">@style/TextAppearance.Github.Headline1</item>
        <item name="textAppearanceHeadline2">@style/TextAppearance.Github.Headline2</item>
        <item name="textAppearanceHeadline3">@style/TextAppearance.Github.Headline3</item>
        <item name="textAppearanceHeadline4">@style/TextAppearance.Github.Headline4</item>
        <item name="textAppearanceHeadline5">@style/TextAppearance.Github.Headline5</item>
        <item name="textAppearanceHeadline6">@style/TextAppearance.Github.Headline6</item>
        <item name="textAppearanceSubtitle1">@style/TextAppearance.Github.Subtitle1</item>
        <item name="textAppearanceSubtitle2">@style/TextAppearance.Github.Subtitle2</item>
        <item name="textAppearanceBody1">@style/TextAppearance.Github.Body1</item>
        <item name="textAppearanceBody2">@style/TextAppearance.Github.Body2</item>
        <item name="textAppearanceButton">@style/TextAppearance.Github.Button</item>
        <item name="textAppearanceCaption">@style/TextAppearance.Github.Caption</item>
        <item name="textAppearanceOverline">@style/TextAppearance.Github.Overline</item>

        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="toolbarStyle">@style/Widget.App.Toolbar</item>
        <item name="bottomNavigationStyle">@style/Widget.App.BottomNavigationView</item>
        <item name="materialCardViewStyle">@style/Widget.App.CardView</item>
    </style>
    <style name="ThemeOverlay.App.BottomNavigationView" parent="">
        <item name="colorSurface">@color/toolbarBackgroundLight</item>
    </style>
    <style name="ThemeOverlay.App.Card" parent="">
        <item name="colorSurface">@color/cardViewSurfaceLight</item>
    </style>
    <style name="Toolbar.TextAppearance" parent="@style/TextAppearance.MaterialComponents.Headline6">
        <item name="android:textColor">@color/toolbarTextLight</item>
        <item name="android:textColorPrimary">@color/toolbarTextLight</item>
    </style>
    <style name="Widget.App.BottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.BottomNavigationView</item>
    </style>
    <style name="Widget.App.CardView" parent="Widget.MaterialComponents.CardView">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Card</item>
    </style>
    <style name="Widget.App.Toolbar" parent="Widget.MaterialComponents.Toolbar">
        <item name="android:textAppearance">@style/Toolbar.TextAppearance</item>
        <item name="titleTextAppearance">@style/Toolbar.TextAppearance</item>
        <item name="android:background">@color/toolbarBackgroundLight</item>
        <item name="colorControlNormal">@color/toolbarTextLight</item>
    </style>
